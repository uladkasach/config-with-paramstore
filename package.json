{
  "name": "config-with-paramstore",
  "version": "1.0.0",
  "description": "a node module that makes it easy to create config files that reference aws paramstore with __PLACEHOLDER__s",
  "main": "src/index.js",
  "scripts": {
    "lint": "echo 'todo'",
    "unit-test": "jest --verbose",
    "test": "npm run lint && npm run unit-test",
    "build": "rm dist/ -rf && babel src -d dist/ --ignore '**/__mocks__/*'",
    "acceptance-test": "npm run build && jest --verbose --config ./acceptance-tests/jest.config.js ./acceptance-tests "
  },
  "author": "UladKasach",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/uladkasach/config-with-paramstore"
  },
  "keywords": [
    "config",
    "aws",
    "paramstore",
    "parameter",
    "store",
    "param",
    "environment",
    "placeholder"
  ],
  "dependencies": {
    "aws-sdk": "^2.305.0",
    "flat": "^4.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.5.0"
  }
}
